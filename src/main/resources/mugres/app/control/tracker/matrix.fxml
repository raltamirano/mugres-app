<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root stylesheets="@arrangement.css" type="javafx.scene.control.ScrollPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <VBox maxWidth="1.7976931348623157E308">
        <HBox>
            <Region HBox.hgrow="ALWAYS" />
            <HBox>
                <children>
                    <Button fx:id="prevParty" text="&lt;" onAction="#previousParty" />
                    <Button fx:id="nextParty" text="&gt;" onAction="#nextParty"/>
                    <Separator orientation="VERTICAL">
                        <padding>
                            <Insets left="5.0" right="5.0" />
                        </padding>
                    </Separator>
                    <Label fx:id="instrumentLabel" minHeight="29.0" text="Instrument">
                        <padding>
                            <Insets right="5.0" />
                        </padding></Label>
                    <ComboBox fx:id="instrumentComboBox" />
                    <Button fx:id="createParty" text="Create" onAction="#createParty" />

                </children>
            </HBox>
            <Region HBox.hgrow="ALWAYS" />
        </HBox>
        <GridPane fx:id="callsMatrix">
            <children>
            </children>
        </GridPane>
    </VBox>
</fx:root>
